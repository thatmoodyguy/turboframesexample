<tr id="<%= dom_id student %>">
  <td class="whitespace-nowrap py-4 pl-4 pr-3 text-sm font-medium text-indigo-700 sm:pl-6"><%= link_to student.first_name, student_path(student), data: {"turbo-frame": "_top"} %></td>
  <td class="whitespace-nowrap px-3 py-4 text-sm text-gray-500"><%= student.last_name %></td>
  <td class="whitespace-nowrap px-3 py-4 text-sm text-gray-500"><%= student.email %></td>
</tr>